{
    "version": 2,
    "builds": [
        {
            "src": "build_files.sh",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "staticfiles"
            }
        },
        {
            "src": "finetuneOfficial/wsgi.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "finetuneOfficial/wsgi.py"
        }
    ],
    "env": {
        "PYTHON_VERSION": "3.9",
        "DJANGO_SETTINGS_MODULE": "finetuneOfficial.settings"
    }
  }
